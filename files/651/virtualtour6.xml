<?xml version="1.0" encoding="UTF-8"?>
<!-- for xml compliance -->
<krpano version="1.0.8.14">
    <!-- actions to perform while loading -->
    <events onxmlcomplete="onxmlcompleteaction();"/>
    <action name="onxmlcompleteaction">
		unselectmapspotsbutpassed();
		selectmapspot(mapspot15,270);
		firstloadcompleteaction();
	</action>
    <action name="playpanosounds">
		playbackgroundsound(none);
	</action>
    <action name="stoppanosounds">
		stopbackgroundsound();
	</action>
    <action name="closepanoobjects">
	</action>
    <!--************** Панорама *******************************************************************-->
    <!-- @File="virtualtour6.xml" @PanoName="doctor" @FilePath="D:/panoramza/ultra fitness/исходники/doctor.tif" -->
    <panoview h="-109.4" v="-0.78" fov="90"/>
    <!-- view parameters -->
    <view fisheye="0" limitview="range" hlookatmin="-180" hlookatmax="180" vlookatmin="-90" vlookatmax="90" maxpixelzoom="1.0" fovmax="90" fov="90" hlookat="-109.4" vlookat="-0.78"/>
    <autorotate horizon="-0.78" tofov="90" waittime="1" speed="5"/>
    <!-- preview image -->
    <preview url="virtualtourdata/virtualtour6/preview.jpg" type="CUBESTRIP" striporder="FRBLUD" details="16"/>
    <!-- the pano itself -->
    <image type="CUBE" multires="true" tilesize="955" baseindex="0">
        <level tiledimagewidth="1910" tiledimageheight="1910">
            <left url="virtualtourdata/virtualtour6/3/1/%v_%u.jpg"/>
            <front url="virtualtourdata/virtualtour6/0/1/%v_%u.jpg"/>
            <right url="virtualtourdata/virtualtour6/1/1/%v_%u.jpg"/>
            <back url="virtualtourdata/virtualtour6/2/1/%v_%u.jpg"/>
            <up url="virtualtourdata/virtualtour6/4/1/%v_%u.jpg"/>
            <down url="virtualtourdata/virtualtour6/5/1/%v_%u.jpg"/>
        </level>
        <level tiledimagewidth="955" tiledimageheight="955">
            <left url="virtualtourdata/virtualtour6/3/0/%v_%u.jpg"/>
            <front url="virtualtourdata/virtualtour6/0/0/%v_%u.jpg"/>
            <right url="virtualtourdata/virtualtour6/1/0/%v_%u.jpg"/>
            <back url="virtualtourdata/virtualtour6/2/0/%v_%u.jpg"/>
            <up url="virtualtourdata/virtualtour6/4/0/%v_%u.jpg"/>
            <down url="virtualtourdata/virtualtour6/5/0/%v_%u.jpg"/>
        </level>
        <!-- mobile phone / iphone images -->
        <mobile>
            <left url="virtualtourdata/virtualtour6/3/mobile_face.jpg"/>
            <front url="virtualtourdata/virtualtour6/0/mobile_face.jpg"/>
            <right url="virtualtourdata/virtualtour6/1/mobile_face.jpg"/>
            <back url="virtualtourdata/virtualtour6/2/mobile_face.jpg"/>
            <up url="virtualtourdata/virtualtour6/4/mobile_face.jpg"/>
            <down url="virtualtourdata/virtualtour6/5/mobile_face.jpg"/>
        </mobile>
        <!-- tablet pc / ipad images -->
        <tablet>
            <left url="virtualtourdata/virtualtour6/3/tablet_face.jpg"/>
            <front url="virtualtourdata/virtualtour6/0/tablet_face.jpg"/>
            <right url="virtualtourdata/virtualtour6/1/tablet_face.jpg"/>
            <back url="virtualtourdata/virtualtour6/2/tablet_face.jpg"/>
            <up url="virtualtourdata/virtualtour6/4/tablet_face.jpg"/>
            <down url="virtualtourdata/virtualtour6/5/tablet_face.jpg"/>
        </tablet>
    </image>
    <!--************** Hotspots and lensflares ****************************************************-->
    <action name="hidepanospotsaction">
	</action>
    <action name="showpanospotsaction">
	</action>
    <!-- close all hotspots -->
</krpano>